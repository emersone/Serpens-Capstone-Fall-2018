    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "patternomaly": "^1.3.2",
    "request": "^2.88.0",
    "session": "^0.1.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "2.2.1",
    "ava": "0.25.0",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": "8.x.x"
  },
  "dendencies": {
    "body-parser": "^1.13.3",
    "express": "^4.16.2",
